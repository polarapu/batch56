Phases:

1) workspace
2) staging / index
3) Local-repo

touch file1
ls
git status
git add file1
git status
git commit -m "label" file1
git status
git log
git show cid

touch file3 file4 file5
ls
git status
git add file3 file4 file5
or
git add *
or
gti add .
or 
git add -A
git status
git commit -m "label"
git status
git log
git show cid

git reset HEAD file-name  (staging to workspace)

git reset --soft cid (local to staging)

git reset --mixed cid  (local to workspace)

git reset --hard cid  (delete)

git clone path/of/c-repo
git clone https://github.com/polarapu/batch56.git
ls
cd batch56
touch prasad
git status
git add *
git commit -m "label"
git push








